{
    "compilerOptions": {
        "baseUrl": "src",
        "target": "es5",
        //    "lib": [
        //      "dom",
        //      "dom.iterable",
        //      "esnext"
        //    ],
        "forceConsistentCasingInFileNames": true,
        "allowJs": true,
        //    "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        //    "forceConsistentCasingInFileNames": true,
        //    "noFallthroughCasesInSwitch": true,
        "module": "ESNext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": false,
        "noEmit": true,
        "jsx": "react-jsx"
    },
    "include": [
        "src"
    ]
}
